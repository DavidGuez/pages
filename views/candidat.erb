<% else %>
    <meta name="twitter:title" content="Soutenez la candidature de <%= candidat['name'] %> à LaPrimaire.org !">
<% end %>
    <meta name="twitter:image" content="<%= candidat['photo'].nil? ? "https://laprimaire.org/assets/images/democratiser-les-elections-logo.jpg": candidat['photo'] %>">
    <meta name="twitter:description" content="Vous adhérez aux idées de <%= candidat['name'] %> et souhaiteriez qu'<%=gender['il']%> joue un rôle plus important pour décider de l'avenir du pays ? Soutenez sa candidature à LaPrimaire.org !">

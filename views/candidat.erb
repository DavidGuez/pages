    <style>
body {
	background: url(https://web.laprimaire.org/img/laprimaire/background-image.jpg) repeat center center fixed;
}
header {
	background:none;
}
h2 span {
	color: #d91d1c;
}
h2.green {
	color:white;
	background-color:green;
	margin-left:-10px;
	width:107%;
	padding:5px;
}
h2.red {
	color:white;
	background-color:#d91d1c;
	padding:5px;
}
h2 {
	font-size: 28px;
}
.modal-title {
	font-size: 20px;
	color:white;
}
.modal-header {
	background-color:#d91d1c;
}
.modal-body {
	font-size: 18px;
}
#candidat_support.qualified {
	border:20px solid green;
}
#candidat_support {
	border:20px solid #d91d1c;
	padding: 10px 0px;
	margin-top:10px;
	background-color:white;
}
#candidat_about {
	border:20px solid #527bdd;
	padding:0px;
	margin-top:-40px;
	background-color:white;
}
#candidat_photo {
	border: 1px solid #5e5e5e;
	margin:20px;
}
#candidat_name {
	font-size: 24px;
	font-weight: bold;
	margin:20px;
}
#candidat_programme p {
	margin-top: 20px;
	font-size: 14px;
}
#candidat_programme {
	margin: 20px;
}
#candidat_soutiens {
	font-size: 15px;
	margin-top:10px;
}
#candidat_legend {
	font-size: 15px;
	margin:15px 0px;
}
#candidat_supportme {
	font-size: 15px;
}
.navbar-default {
	border-bottom:40px solid #d91d1c;
}
.donateurs .goalProgress {
	/*background: #527bdd;*/
	background: white;
	border:1px solid #BBB;
}
.donateurs div.progressBar {
	/*background: #d91d1c;*/
	background: #56d840;
	overflow:visible;
	height:32px;
	color: #5e5e5e;
	text-align:left;
	font-size: 22px;
	letter-spacing: -1px;
	padding-left: 20px;
	padding-top: 2px;
	display: block;
	width: 20px;
}
span.goalAmount {
	display: none;
	text-indent: -9999px;
}

footer {
	position: relative;
	padding: 10px;
	margin-top: 40px;
	font-size: 18px;
}
.join-link {
	font-size: 18px;
	font-weight: bold;
}
#slogan {
	float: right !important;
	color:#5e5e5e;
	font-weight:bold;
	font-size:18px;
	margin-top: 0px;
	font-variant:small-caps;
	margin-right: 20px;
}
.about-lp {
	text-align:center;
	padding-top:0px;
	padding-bottom:10px;
}
@media (min-width: 768px) {
	#slogan {
		float: right !important;
		color:#5e5e5e;
		font-weight:bold;
		font-size:28px;
		margin-top: 20px;
		font-variant:small-caps;
		margin-right: 20px;
	}
	.join-link {
		font-size: 20px;
	}
	.about-lp {
		text-align:right;
		padding-top:10px;
	}
}
@media (min-width: 1280px) {
	footer {
		font-size: 20px;
		position:fixed;
		padding: 0px;
	}
	#candidat_support {
		margin-left:-60px;
	}
	.about-lp {
		padding-bottom:0px;
		padding-top:7px;
	}
}
    </style>

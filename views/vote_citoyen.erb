				$('#votebtn_'+voteId+'_error').show();
			} else if (data['message']=='voteCanceled') {
				$('.modal.cocorico').modal('hide');
			}
		};
		$('#vote_tutorial').on('show.bs.modal', function () {
			//$('#vote_tutorial_iframe').attr("src",'/citoyen/vote/tutorial'); // 1er TOUR
			$('#vote_tutorial_iframe').attr("src",'/citoyen/vote/tutorial_final'); // 2nd TOUR
			$('#vote_tutorial_iframe').show();
		});
	    if ($('#<%= vote['vote_id']%>_cc')!=null) {
		    $('#<%= vote['vote_id']%>_cc').on('hidden.bs.modal', function () {
				    $('#<%= vote['vote_id']%>_loading').show();
				    $('#<%= vote['vote_id']%>_iframe').hide();
				    $('#<%= vote['vote_id']%>_iframe').attr("src",'about:blank');
				    });

		    $('#<%= vote['vote_id']%>_cc').on('shown.bs.modal', function () {
				    $('#<%= vote['vote_id']%>_iframe').load(function() { $('#<%= vote['vote_id']%>_loading').hide(); $('#<%= vote['vote_id']%>_iframe').show();});
				    $.ajax({
					url: "/citoyen/token/<%= citoyen['user_key'] %>",
					data: "ballot=<%= ballot['ballot_id'] %>&vote_id=<%= vote['vote_id'] %>",
					dataType: "json",
					success: function(data) {
						$('#<%= vote['vote_id']%>_iframe').attr("src",'https://<%= COCORICO_HOST %>/embed/vote-widget/<%= vote['cc_vote_id'] %>?user='+data['token']+'&appId=<%= cocorico_app_id %>&locale=fr-FR');
						},
					error: function(data) {
						console.log("votes are paused. reloading page...");
						window.location.reload(true);
						}
					});
		    });
	}
});
    </script>
    <script src="https://use.fontawesome.com/1e37fa74cb.js"></script>

  </body>
</html>

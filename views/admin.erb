	min-height: 300px;
	padding:0px;
}
.candidate_bio {
	overflow: hidden;
	position: relative;
	margin: 0 auto;
	border: 8px solid #d91d1c;
	border-left: 2px solid #7e7e7e;
	height: 300px;
	padding:0px;
}
.badge_soutiens {
	position:absolute;
	color:white;
	font-size:12px;
	background-color:#d91d1c;
	display:none;
	padding:5px;
	top:0px;
	z-index: 1;
}
.badge_qualified {
	position:absolute;
	color:white;
	font-size:12px;
	font-weight:bold;
	background-color:#56d840;
	padding:5px;
	top:0px;
	right:0px;
	z-index: 1;
}
.badge_qualified.non { display:none; }
.nb_soutiens_total {
	position:absolute;
	color:white;
	font-size:12px;
	background-color:#d91d1c;
	padding:5px;
	bottom:0px;
	z-index: 1;

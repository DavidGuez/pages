    <script type="text/javascript">
    	// validation du formulaire email
			$(document).ready(function(){
				if ($('#valid_email_form').length) {
				$('#valid_email_form').submit(function(e) {
					var email=$('#valid_email_input').val();
					console.log(email);
					$.post("https://laprimaire.ngrok.io/v1/citizen/email",{'email':email,'key':'<%= citoyen['user_key'] %>'})
					.done(function( data ){
						json=$.parseJSON(data);
						$('#valid_email_input').html('<img src="https://maps.googleapis.com/maps/api/staticmap?size=250x200&maptype=roadmap\&markers=size:mid|color:red|'+json['city']+'+'+json['zipcode']+'+'+json['country']+'&key=AIzaSyBkz-bknBrwrE9RXtqHGu2AuR_95WVNGc8">');
				});
					e.preventDefault();
					})
				}
			});

			// submit du formulaire du numéro de telephone
			$(document).ready(function() {
				$('#valid_phone_form').submit(function(e) {
					e.preventDefault();
					if (validPhoneNumber($('#valid_phone_input').val())) {
						tel = $('#valid_phone_input').val();
						$.post("http://localhost:9292/v1/citizen/tel",{'tel':tel})
						.done(function(msg){
							$("#my_phone").modal('toggle');
							$("#my_phone_code").modal('toggle');
						})
    				.fail(function(xhr, status, error) {
    			    var jsonResponse = JSON.parse(xhr.responseText);
    			    var errorsHtml = '<div class="alert alert-danger">' + jsonResponse.error + '</div>';
			        $( '#valid_phone_form_errors' ).html( errorsHtml );
			      });
					} else {
						var errorsHtml = '<div class="alert alert-danger">Le numéro de téléphone saisi n\'est pas valide</div>';
				     $( '#valid_phone_form_errors' ).html( errorsHtml );
					}
				});
			});

			// submit du formulaire du code de confirmation du telephone
			$(document).ready(function(){
				if ($('#valid_phone_code_input').length) {
					$('#valid_phone_code_form').submit(function(e) {
						e.preventDefault();
						tel = $('#valid_phone_code_input').val();
						//$.post("http://localhost:9292/v1/citizen/tel",{'tel':tel});
					});
				}
			});

			function validPhoneNumber(phoneNumber) {
				if (!phoneNumber || 0 === phoneNumber.length) {
					return false;
				}
				var reg_mobile = '^\\+33(6|7)[0-9]{8}$';

				if( phoneNumber.match(reg_mobile) ){
					return true;
				}
				return false;
			}
    </script>
